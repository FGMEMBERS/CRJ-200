<?xml version="1.0"?>

<PropertyList>

<fx>
<engine>
<name>left engine</name>
<path>Sounds/jet.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[0]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>0</value>
</equals>
<not>
<property>sim/crashed</property>
</not>
</and>
</condition>
<volume>
<property>/engines/engine[0]/thrust-lbs</property>
<factor>0.0004</factor>
<min>0.0</min>
<max>0.8</max>
</volume>
<position>
<x>-1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<engine>
<name>left n1</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[0]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>0</value>
</equals>
<not>
<property>/sim/crashed</property>
</not>
</and>
</condition>
<volume>
<factor>0.4</factor>
</volume>
<pitch>
<property>/engines/engine[0]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>0.8</max>
</pitch>
<position>
<x>-1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<engine>
<name>right engine</name>
<path>Sounds/jet.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[1]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>0</value>
</equals>
<not>
<property>/sim/crashed</property>
</not>
</and>
</condition>
<volume>
<property>/engines/engine[1]/thrust-lbs</property>
<factor>0.0004</factor>
<min>0.0</min>
<max>0.8</max>
</volume>
<position>
<x>1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<engine>
<name>right n1</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<greater-than>
<property>/sim[0]/time/elapsed-sec</property>
<value>10</value>
</greater-than>
<property>/engines/engine[1]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>0</value>
</equals>
<not>
<property>/sim/crashed</property>
</not>
</and>
</condition>
<volume>
<factor>0.4</factor>
</volume>
<pitch>
<property>/engines/engine[1]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>0.8</max>
</pitch>
<position>
<x>1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<engine>
<name>left engine inside</name>
<path>Sounds/jet.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[0]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>1</value>
</equals>
<not>
<property>/sim/crashed</property>
</not>
</and>
</condition>
<volume>
<property>/engines/engine[0]/thrust-lbs</property>
<factor>0.0002</factor>
<min>0.0</min>
<max>0.3</max>
</volume>
<position>
<x>-1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<engine>
<name>left n1 inside</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[0]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>1</value>
</equals>
<not>
<property>sim/crashed</property>
</not>
</and>
</condition>
<volume>
<factor>0.1</factor>
</volume>
<pitch>
<property>/engines/engine[0]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>1.0</max>
</pitch>
<position>
<x>-1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<engine>
<name>right engine inside</name>
<path>Sounds/jet.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[1]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>1</value>
</equals>
<not>
<property>sim/crashed</property>
</not>
</and>
</condition>
<volume>
<property>/engines/engine[1]/thrust-lbs</property>
<factor>0.0002</factor>
<min>0.0</min>
<max>0.3</max>
</volume>
<position>
<x>1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<engine>
<name>right n1 inside</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[1]/running</property>
<equals>
<property>/sim/current-view/internal</property>
<value>1</value>
</equals>
<not>
<property>sim/crashed</property>
</not>
</and>
</condition>
<volume>
<factor>0.1</factor>
</volume>
<pitch>
<property>/engines/engine[1]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>1.0</max>
</pitch>
<position>
<x>1.3</x>
<y>0.3</y>
<z>10.0</z>
</position>
</engine>

<rumble>
<name>rumble</name>
<mode>looped</mode>
<path>Sounds/rumble.wav</path>
<condition>
<or>
<property>/gear/gear[0]/wow</property>
<property>/gear/gear[1]/wow</property>
<property>/gear/gear[2]/wow</property>
</or>
</condition>
<volume>
<property>/velocities/airspeed-kt</property>
<type>log</type>
<factor>0.5</factor>
<max>1</max>
</volume>
</rumble>

<gear1>
<name>gear</name>
<mode>in-transit</mode>
<path>Sounds/gear.wav</path>
<property>/gear/gear[0]/position-norm</property>
<pitch>
<internal>dt_play</internal>
<offset>1.0</offset>
<factor>-0.001</factor>
</pitch>
</gear1>

<gear1>
<name>gear-lock</name>
<path>Sounds/gear-lck.wav</path>
<condition>
<equals>
<property>/gear/gear[0]/position-norm</property>
<value>0</value>
</equals>
</condition>
</gear1>

<squeal1>
<name>squeal</name>
<path>/Aircraft/CRJ-200/Sounds/touchdown.wav</path>
<property>/gear/gear[0]/wow</property>
<volume>
<property>/velocities/speed-down-fps</property>
<factor>0.05</factor>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
</volume>
<volume>
<internal>dt_stop</internal>
<factor>0.05</factor>
<max>1.0</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.0025</factor>
<offset>1.2</offset>
</pitch>
</squeal1>

<gear2>
<name>gear</name>
<mode>in-transit</mode>
<path>Sounds/gear.wav</path>
<property>/gear/gear[1]/position-norm</property>
<pitch>
<internal>dt_play</internal>
<offset>1.0</offset>
<factor>-0.001</factor>
</pitch>
</gear2>

<gear2>
<name>gear-lock</name>
<path>Sounds/gear-lck.wav</path>
<condition>
<equals>
<property>/gear/gear[1]/position-norm</property>
<value>0</value>
</equals>
</condition>
</gear2>

<squeal2>
<name>squeal</name>
<path>/Aircraft/CRJ-200/Sounds/touchdown.wav</path>
<property>/gear/gear[1]/wow</property>
<volume>
<property>/velocities/speed-down-fps</property>
<factor>0.05</factor>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
</volume>
<volume>
<internal>dt_stop</internal>
<factor>0.05</factor>
<max>1.0</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.0025</factor>
<offset>1.2</offset>
</pitch>
</squeal2>

<gear3>
<name>gear</name>
<mode>in-transit</mode>
<path>Sounds/gear.wav</path>
<property>/gear/gear[2]/position-norm</property>
<pitch>
<internal>dt_play</internal>
<offset>1.0</offset>
<factor>-0.001</factor>
</pitch>
</gear3>

<gear3>
<name>gear-lock</name>
<path>Sounds/gear-lck.wav</path>
<condition>
<equals>
<property>/gear/gear[2]/position-norm</property>
<value>0</value>
</equals>
</condition>
</gear3>

<squeal3>
<name>squeal</name>
<path>/Aircraft/CRJ-200/Sounds/touchdown.wav</path>
<property>/gear/gear[2]/wow</property>
<volume>
<property>/velocities/speed-down-fps</property>
<factor>0.05</factor>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
</volume>
<volume>
<internal>dt_stop</internal>
<factor>0.05</factor>
<max>1.0</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.0025</factor>
<offset>1.2</offset>
</pitch>
</squeal3>

<flap>
<name>flaps</name>
<mode>in-transit</mode>
<path>Aircraft/CRJ-200/Sounds/Flaps.wav</path>
<property>/surface-positions/flap-pos-norm</property>
<volume>
<factor>0.2</factor>
</volume>
</flap>

<wind>
<name>wind</name>
<mode>looped</mode>
<path>Sounds/wind.wav</path>
<property>/velocities/airspeed-kt</property>
<volume>
<property>/position/altitude-ft</property>
<factor>-0.000015</factor>
<offset>1.0</offset>
<min>0.1</min>
<max>1.0</max>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.0015</factor>
<min>0.03</min>
<max>0.25</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
<offset>1.25</offset>
</pitch>
</wind>

<stall>
  <name>stall-1</name>
  <mode>looped</mode>
  <path>/Aircraft/CRJ-200/Sounds/stall.wav</path>
  <condition>
    <and>
      <greater-than>
        <property>/sim[0]/time/elapsed-sec</property>
        <value>10</value>
      </greater-than>
      <less-than>
        <property>/velocities/airspeed-kt</property>
        <value>140</value>
      </less-than>
      <less-than>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.015</value>
      </less-than>
      <equals>
	<property>/sim/current-view/cockpit</property>
	<value>1</value>
      </equals>
      <not>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
          <property>sim/crashed</property>
        </or>
      </not>
    </and>
  </condition>
  <volume>
    <factor>0.8</factor>
  </volume>
</stall>

<stall>
  <name>stall-2</name>
  <mode>looped</mode>
  <path>/Aircraft/CRJ-200/Sounds/stall.wav</path>
  <condition>
    <and>
      <greater-than>
        <property>/sim[0]/time/elapsed-sec</property>
        <value>10</value>
      </greater-than>
      <less-than>
        <property>/velocities/airspeed-kt</property>
        <value>135</value>
      </less-than>
      <greater-than>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.015</value>
      </greater-than>
      <less-than>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.065</value>
      </less-than>
      <equals>
	<property>/sim/current-view/cockpit</property>
	<value>1</value>
      </equals>
      <not>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
          <property>sim/crashed</property>
        </or>
      </not>
    </and>
  </condition>
  <volume>
    <factor>0.8</factor>
  </volume>
</stall>

<stall>
  <name>stall-3</name>
  <mode>looped</mode>
  <path>/Aircraft/CRJ-200/Sounds/stall.wav</path>
  <condition>
    <and>
      <greater-than>
        <property>/sim[0]/time/elapsed-sec</property>
        <value>10</value>
      </greater-than>
      <less-than>
        <property>/velocities/airspeed-kt</property>
        <value>120</value>
      </less-than>
      <greater-than>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.065</value>
      </greater-than>
      <equals>
	<property>/sim/current-view/cockpit</property>
	<value>1</value>
      </equals>
      <not>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
          <property>sim/crashed</property>
        </or>
      </not>
    </and>
  </condition>
  <volume>
    <factor>0.8</factor>
  </volume>
</stall>

		<crash>
			<name>intense ground contact</name>
			<mode>transient</mode>
			<path>Sounds/crash.wav</path>
			<property>sim/crashed</property>
			<volume>
				<factor>1</factor>
			</volume>
			<pitch>
				<factor>1.5</factor>
			</pitch>
		</crash>

<switch>
   <name>click on</name>
   <mode>once</mode>
   <path>Aircraft/CRJ-200/Sounds/click.wav</path>
   <condition>
<property>/sim/sound/switch</property>
</condition>
   <volume>
    <factor>0.45</factor>
   </volume>
  </switch>

<switch>
   <name>click off</name>
   <mode>once</mode>
   <path>Aircraft/CRJ-200/Sounds/click.wav</path>
   <condition>
   <not>
<property>/sim/sound/switch</property>
</not>
</condition>
   <volume>
    <factor>0.45</factor>
   </volume>
  </switch>


<engines>
  <name>reverse-thrust inside</name>
  <mode>looped</mode>
  <path>Sounds/exhaust_loop.wav</path>
  <condition>
    <and>
      <equals>
        <property>/engines/engine[0]/running</property>
        <value>1</value>
      </equals>
      <equals>
        <property>/controls/engines/engine[0]/reverser</property>
        <value>1</value>
      </equals>
<equals>
<property>/sim/current-view/internal</property>
<value>1</value>
</equals>
      <not>
        <property>sim/crashed</property>
      </not>
    </and>
  </condition>
  <volume>
    <property>surface-positions/reverser-pos-norm</property>
    <factor>1.0</factor>
  </volume>
  <volume>
    <property>/engines/engine[1]/thrust-lbs</property>
    <factor>0.0002</factor>
    <min>0.0</min>
    <max>0.6</max>
  </volume>
</engines>

<engines>
<name>reverse-thrust outside</name>
<mode>looped</mode>
<path>Sounds/exhaust_loop.wav</path>
<condition>
<and>
<equals>
<property>/engines/engine[0]/running</property>
<value>1</value>
</equals>
<equals>
<property>/controls/engines/engine[0]/reverser</property>
<value>1</value>
</equals>
<equals>
<property>/sim/current-view/internal</property>
<value>0</value>
</equals>
<not>
<property>sim/crashed</property>
</not>
</and>
</condition>
  <volume>
    <property>surface-positions/reverser-pos-norm</property>
    <factor>1.0</factor>
  </volume>
<volume>
<property>/engines/engine[1]/thrust-lbs</property>
<factor>0.0002</factor>
<min>0.0</min>
<max>1.0</max>
</volume>
</engines>

</fx>

</PropertyList>
